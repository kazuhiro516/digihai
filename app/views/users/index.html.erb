<div class="container">
  <h1 class="text-center">全てのユーザー</h1>
  <ul class="users">
    <% @users.each do |user| %>
      <li>
        <%= image_tag avatar_url(user), class: "round-img"%>
        <%= link_to user.name, user %>
      </li>
    <% end %>
  </ul>
  <div class="pagination justify-content-center my-5">
    <%= paginate @users %>
  </div>
</div>
